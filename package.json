{
  "name": "datamind",
  "version": "1.0.0",
  "description": "NDMarket AI Insight Platform - NestJS Backend",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "build:frontend": "cd frontend && yarn build",
    "build:all": "yarn build:frontend && yarn build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "concurrently -n \"Backend,Frontend\" -c \"bgBlue.bold,bgMagenta.bold\" \"nest start --watch\" \"cd frontend && yarn dev\"",
    "dev:frontend": "cd frontend && yarn dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:frontend": "cd frontend && yarn lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "embed:sql-examples": "ts-node scripts/embed-sql-examples.ts",
    "verify:embeddings": "ts-node scripts/verify-embeddings.ts",
    "test:rag": "ts-node -r tsconfig-paths/register scripts/test-rag.ts"
  },
  "author": "blueng <blueng.choi@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.933.0",
    "@aws-sdk/client-secrets-manager": "^3.933.0",
    "@aws-sdk/credential-providers": "^3.936.0",
    "@langchain/aws": "^1.1.0",
    "@langchain/community": "^1.0.4",
    "@langchain/core": "^1.0.5",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/langgraph-supervisor": "^1.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "aws4": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "langchain": "^1.0.5",
    "mysql2": "^3.15.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@aws-sdk/client-sts": "^3.936.0",
    "@nestjs/cli": "^11.0.10",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/ansi-colors": "^3.2.6",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "ansi-colors": "^4.1.3",
    "cli-progress": "^3.12.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
